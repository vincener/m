	"cateVodId": "/div/a/@href",
	"cateVodIdR": "/show-(\\w+).html",
	"cateVodImg": "/i[@class='icon-play']/@data-src",
	"cateVodMark": "/div[@class='module-item-caption']/text()",
    // 详情页地址 用于获取详情页信息 及 播放列表和地址
	"dtUrl": "https://www.dianyi.ng/show-{vid}.html",
    // 详情节点
	"dtNode": "//div[contains(@class,'box view-heading')]",
    // 视频名
	"dtName": "//h1[contains(@class,'page-title')]/text()",
	"dtNameR": "",
    // 视频图片
	"dtImg": "//div[contains(@class,'mobile-play')]/div/@data-src",
	"dtImgR": "",
    // 视频分类
	"dtCate": "//span[@class='video-info-itemtitle' and contains(text(), '类型：')]/following-sibling::*/text()",
	"dtCateR": "",
    // 视频年份
	"dtYear": "//span[@class='video-info-itemtitle' and contains(text(), '年份：')]/following-sibling::*/text()",
    // 视频简介
	"dtDesc": "//div[contains(@class,'vod_content')]/span/text()",
	"dtDescR": "",
    // 播放源节点
	"dtFromNode": "//div[@class='module-tab-item tab-item']/span",
    // 播放源名称
	"dtFromName": "//text()",
